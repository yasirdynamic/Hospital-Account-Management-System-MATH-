@model FinalMath.Models.USER


@if (Session["Admin"] != null)
{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
else
{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutDirector.cshtml";
}

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Users</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">Users</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>




<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h2>Delete</h2>
                    </div>
                    <div class="card-body">

                        <h3>Are you sure you want to delete this?</h3>

                        <div>
                            <dl class="dl-horizontal">
                                <dt>
                                    @Html.DisplayNameFor(model => model.USER_NAME)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.USER_NAME)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.USER_PASSWORD)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.USER_PASSWORD)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.IsActive)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.IsActive)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.EMPLOYEE.EMPLOYEE_NAME)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.EMPLOYEE.EMPLOYEE_NAME)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.USERTYPE.USER_TYPE)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.USERTYPE.USER_TYPE)
                                </dd>

                            </dl>

                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                                <div class="form-actions no-color">
                                    <input type="submit" value="Delete" class="btn btn-sm btn-danger" />
                                    <a href="@Url.Action("Index", "USER")" class="btn btn-sm btn-primary">Back to List</a>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
