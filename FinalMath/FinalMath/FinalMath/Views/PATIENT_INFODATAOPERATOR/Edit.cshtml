@model FinalMath.Models.PATIENT_INFO


@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutDataOperator.cshtml";
}

<script type="text/javascript">
    window.onload = function () {
        var vSkill = document.getElementById('CHRONIC_MEDICAL_PROBLEM');

        var vSkillText = vSkill.options[vSkill.selectedIndex].innerHTML;

        if (vSkillText == "No") {

            $("#hidden_div").hide();
        }
        else {

            $("#hidden_div").show();
        }


        var vSkill = document.getElementById('HAVING_EVER_BEEN_ARRESTED_AND_CHARGED_FOR_ANY_CRIME');

        var vSkillText = vSkill.options[vSkill.selectedIndex].innerHTML;

        if (vSkillText == "No") {

            $("#hidden_div1").hide();
        }
        else {

            $("#hidden_div1").show();
        }


        var vSkill = document.getElementById('LIVES_WITH_ANY_ONE_WHO_ABUSES_DRUGS_ALCOHAL');

        var vSkillText = vSkill.options[vSkill.selectedIndex].innerHTML;

        if (vSkillText == "No") {

            $("#hidden_div2").hide();
        }
        else {

            $("#hidden_div2").show();
        }



        var vSkill = document.getElementById('YOU_HAD_EXPERIENCED_SERIOUS_PROBLEM_WITH_YOUR_FAMILY_OTHER_SIGNIFICANT_FAMILY');

        var vSkillText = vSkill.options[vSkill.selectedIndex].innerHTML;

        if (vSkillText == "No") {

            $("#hidden_div3").hide();
        }
        else {

            $("#hidden_div3").show();
        }
    };
</script>
